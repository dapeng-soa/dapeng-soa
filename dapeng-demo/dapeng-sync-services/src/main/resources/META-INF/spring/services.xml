<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:soa="http://soa-springtag.dapeng.com/schema/service"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://soa-springtag.dapeng.com/schema/service
    http://soa-springtag.dapeng.com/schema/service/service.xsd">

    <bean id="printService" class="com.github.dapeng.soa.PrintServiceImpl"/>
    <soa:service ref="printService"/>

    <bean id="calculateService" class="com.dapeng.soa.CalculateServiceImpl"/>
    <soa:service ref="calculateService"/>

</beans>